# Server Configuration
NODE_ENV=development
PORT=3001
API_PREFIX=/api

# Database
DATABASE_URL=postgresql://delivery_user:yourpassword@localhost:5432/user_service_db

# JWT Secrets (Change these in production!)
JWT_SECRET=your_super_secret_jwt_key_change_in_production_12345
JWT_EXPIRES_IN=15m
JWT_REFRESH_SECRET=your_super_secret_refresh_key_change_in_production_67890
JWT_REFRESH_EXPIRES_IN=7d

# RabbitMQ
RABBITMQ_URL=amqp://localhost:5672
RABBITMQ_EXCHANGE=delivery_exchange
RABBITMQ_QUEUE_USER=user_queue
